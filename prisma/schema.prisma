// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Paciente{
  cedula Int @id
  nombre String
  apellido Spring
  fechaNacimiento DateTime
  telefono String
}

model Medico{
  tarjetaProfesional Int @id
  nombre String
  apellido Spring
  fechaNacimiento DateTime
  correo String
  IdSpecialidad Int 
}

model Cita{
  idCita Int @id
  fecha DateTime
  cedulaPaciente Int
  tarjetaProfesional Int
}

model Especialidad{
  IdSpecialidad Int @id
  nombre Spring
    
}
